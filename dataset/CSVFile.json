{
	"name": "CSVFile",
	"properties": {
		"linkedServiceName": {
			"referenceName": "BlobStorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileName": {
				"type": "string"
			},
			"DirectoryName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().FileName",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().DirectoryName",
					"type": "Expression"
				},
				"container": "exported-data"
			},
			"columnDelimiter": "|^|",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}